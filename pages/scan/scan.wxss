/* pages/china/china.wxss */
page{
  background-color:#CDE0C9;
  display:flex;
  flex-direction:column;
  justify-content:flexstart;
 }
 .head{
   background-color: whitesmoke;
   width:98%;
   border-style: solid;
   border-width: 2px;
    border-radius: 5px;
    border-color: gray;
 }
 .fah{
   vertical-align: middle;
   position:relative;
    width:200px; 
    height:20px;
  border-width: 1px;
   border-radius: 5px;
   border-color: gray;
   float:left;
 }
 .son{
   float:left;
   font-size: 15px;
   font-weight:normal;
 }
 .son1{
   position: absolute;
   top: 10%;
   right: 10%;
 }
 .text{
   font-size: 17px;
   font-weight:lighter;
 }
 .interval{
   width:100%;
   height:5px;
 }
 .content{
   border-style:solid;
   border-width:1px;
   border-radius:5px;
   border-color:gray;
   background-color: white;
   width:96%;
   opacity:0.7;
 }
 .content1{
   display:flex;
   align-items: center;
   justify-content: center;
 }
 #change{
   position: relative;
  margin-top:10px;
   top:10px;
 }
 .title1{
   font-size:22px;
   text-align:center;
   font-weight:bold;
   color:whitesmoke;
   text-shadow: 0 0px 5px gray;
 }
 
 .dot {
    height: 10px;
		width: 10px;
		background-color: rgba(171, 226, 223);
    border-radius: 50%;

}
 
 
 .biaodian{
   width:3px;
   height:3px;
   border-color: black;
   background-color:black;
 }